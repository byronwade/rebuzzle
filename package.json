{
	"name": "rebuzzle",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"packageManager": "npm@7.24.0",
	"devDependencies": {
		"turbo": "latest"
	},
	"scripts": {
		"dev": "turbo run dev",
		"build": "turbo run build",
		"dev:web": "turbo run dev --filter=web",
		"dev:desktop": "turbo run dev --filter=desktop",
		"dev:mobile": "turbo run dev --filter=mobile"
	}
}